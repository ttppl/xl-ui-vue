{
  "name": "xl-ui-vue",
  "version": "0.2.4",
  "description": "基于 vue3 的 UI 组件库",
  "author": "lzx",
  "main": "lib/index/index.js",
  "style": "lib/index/style.css",
  "keywords": [
    "xl-ui"
  ],
  "private": false,
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "buildc": "vue-cli-service build",
    "build": "vue-cli-service build --target lib --name xl-ui --dest lib packages/index.js",
    "build:theme": "vue-cli-service build --target lib --name xl-ui --dest lib packages/styles/setting.less",
    "build:cmp": "vue-cli-service build --target wc-async --name xl 'packages/input/src/*.vue'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "less": "^4.1.1",
    "less-loader": "^6.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}